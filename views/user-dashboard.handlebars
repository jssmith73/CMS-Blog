{{#each posts}}
<h2 class="title-headers">{{title}}</h2>
<div id="post-body">
    <a href="/post/{{id}}">
        <h3>{{content}}</h3>
        <p id="author-date">{{date_created}}</p>
    </a>
</div>
{{/each}}

<button class="btn" id="new-post"><a href="/dashboard/new-post">New Post</a></button>

<button class="btn" id="post-edit"><a href="/dashboard/edit-post">Edit Post</a></button>
